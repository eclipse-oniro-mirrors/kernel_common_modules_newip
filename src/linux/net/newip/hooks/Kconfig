config NEWIP_HOOKS
	bool "NEWIP HOOKS"
	default n
	depends on VENDOR_HOOKS && NEWIP
	help
	  Enable NewIP hooks implemented as tracepoints
	  Allow NewIP modules to attach to tracepoint "hooks" defined via
	  DECLARE_TRACE or DECLARE_HOOK

